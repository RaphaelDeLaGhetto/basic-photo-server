<% include ../_partials/head %>
  <% include ../_partials/messages %>
  <main>
    <% include ../_partials/appLink %>
    <% if (!images.length) { %>
      <h2>No invoices</h2>
    <% } else { %>
      <% if (canWrite) { %>
        <section class="copy">
          <h1><a href="<%= `/image/${agent.getAgentDirectory()}/zip` %>">Zip and download all invoices</a><h1>
        </section>
      <% } %>
      <% include ./_pager %>
      <% for (let image of images) { %>
        <section class="image">
          <a href="<%= `/image/${image}` %>">
            <img src="<%= `/uploads/${image}` %>" />
          </a>
        </section>
      <% } %>
      <% include ./_pager %>
    <% } %>
  </main> 
<% include ../_partials/footer %>

